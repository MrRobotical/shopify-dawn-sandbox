<h1>{{ section.settings.text }}</h1>
<h2>{{ section.settings.richtext }}</h2>

{% schema %}
  {
    "name": "Jays Custom Section",
    "settings": [
      {
        "type":"text",
        "id": "text",
        "label": "Text Input"
      },
      {
        "type":"richtext",
        "id": "richtext",
        "label": "Rich Text Input"
      },
      {
        "type": "checkbox",
        "id": "show_announcement",
        "label": "Show This Announcement",
        "default": true
      }
    ],
    "presets": [{"name": "Default custom section"}],
    "tag": "header",
    "class": "custom-section",
    "limit": 1
  }



  
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

